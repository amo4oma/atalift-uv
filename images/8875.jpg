    <script>
    document.addEventListener("DOMContentLoaded", function() {
        new Glide('#sbha-carousel-reviews', {
            type: 'slider',
            startAt: 0,
            perView: 1,
            focusAt: 'center',
            gap: 50,
        }).mount();

        new Glide('.section-related-products-container-mobile-carousel', {
            type: 'slider',
            startAt: 0,
            perView: 1,
            focusAt: 'center',
            gap: 30
        }).mount();

        const comparisonImages = document.getElementsByClassName('prod-comparison-image');
        if (comparisonImages) {
            const delay = Math.round(Math.random() * (500 - 50)) + 50;

            Array.from(comparisonImages).forEach(function(element) {
                const target = element;
                setTimeout(function() {
                    target.src = target.getAttribute("data-src");
                }, delay);
            });
        }

        var scrollOffset = jQuery('.page-section')[0].offsetTop + 5;
        jQuery(window).scroll(function() {
            const scrollDistance = jQuery(window).scrollTop();

            // Assign active class to nav links while scolling
            jQuery('.page-section').each(function(i) {
                if ((jQuery(this).position().top - scrollOffset) <= scrollDistance) {
                    jQuery('.header-button').removeClass('active');
                    jQuery('.header-button').eq(i).addClass('active');
                }
            });
        }).scroll();
    });

    function onClickHamburger(e) {
        if (e) {
            e.preventDefault();
        }

        const hamburgerIcon = document.getElementsByClassName('hamburger-icon');
        if (hamburgerIcon && hamburgerIcon.length > 0) {
            if (hamburgerIcon[0].className.includes('open')) {
                hamburgerIcon[0].className = hamburgerIcon[0].className.replace(' open', '');
            } else {
                hamburgerIcon[0].className += ' open';
            }
        }

        const headerContentMobile = document.getElementsByClassName('header-content-small');
        if (headerContentMobile && headerContentMobile.length > 0) {
            if (headerContentMobile[0].className.includes('open')) {
                headerContentMobile[0].className = headerContentMobile[0].className.replace(' open', '');
            } else {
                headerContentMobile[0].className += ' open';
            }
        }
    }

    function onClickMenu(e) {
        if (e.stopPropagation) {
            e.stopPropagation();
        } else {
            window.event.cancelBubble = true;
        }
        e.preventDefault();


        const hamburgerIcon = document.getElementsByClassName('hamburger-icon');
        if (hamburgerIcon && hamburgerIcon.length > 0) {
            hamburgerIcon[0].className = hamburgerIcon[0].className.replace(' open', '');
        }

        const headerContentMobile = document.getElementsByClassName('header-content-small');
        if (headerContentMobile && headerContentMobile.length > 0) {
            headerContentMobile[0].className = headerContentMobile[0].className.replace(' open', '');
        }

        const sectionId = e.currentTarget.getAttribute('data-src');

        const section = document.getElementById(sectionId);
        if (section) {
            section.scrollIntoView({
                behaviour: 'smooth'
            });
        }
    }

    function onClickTabItem(e) {
        const tabItems = document.getElementsByClassName('steps-tab-item');
        for (let i = 0; i < tabItems.length; i++) {
            tabItems[i].className = tabItems[i].className.replace(' active', '');
        }
        e.currentTarget.className += ' active';

        const tabContents = document.getElementsByClassName('steps-content-container');
        for (let i = 0; i < tabContents.length; i++) {
            tabContents[i].className = tabContents[i].className.replace(' active', '');
        }

        const tabContent = document.getElementById(e.currentTarget.getAttribute('data-tab'));
        tabContent.className += ' active';
    }

    function onClickFAQItem(e) {
        const item = e.currentTarget;
        const answerContainers = item.getElementsByClassName('faq-answer-container');
        if (answerContainers.length > 0) {
            const answerContainer = answerContainers[0];
            if (answerContainer.style.maxHeight) {
                answerContainer.style.maxHeight = null;
            } else {
                answerContainer.style.maxHeight = answerContainer.scrollHeight + "px";
            }
        }

        const decorationIcons = item.getElementsByClassName('faq-decoration-icon');
        if (decorationIcons.length > 0) {
            const decorationIcon = decorationIcons[0];
            if (decorationIcon.className.includes('active')) {
                decorationIcon.className = decorationIcon.className.replace(' active', '');
            } else {
                decorationIcon.className += ' active';
            }
        }

        const indicatorIcons = item.getElementsByClassName('faq-indicator-icon');
        if (indicatorIcons.length > 0) {
            const indicatorIcon = indicatorIcons[0];
            if (indicatorIcon.className.includes('active')) {
                indicatorIcon.className = indicatorIcon.className.replace(' active', '');
            } else {
                indicatorIcon.className += ' active';
            }
        }
    }
    </script>

